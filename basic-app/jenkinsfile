pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                script {
                    sh 'npm install'
                }
            }
        }
        stage('Test') {
            steps {
                script {
                    sh 'npm test'  // Agar test hain to run karein, nahi to is step ko hata sakte hain
                }
            }
        }
        stage('Build Docker Image') {
            steps {
                script {
                    sh 'docker build -t amitkasera2001/ci-cd-pipeline-sample .'
                }
            }
        }
       
        
    }
}
